ated":{"line":111,"column":26},"source":"components/App.js","original":{"line":40,"column":24}},{"name":"tracks","generated":{"line":111,"column":27},"source":"components/App.js","original":{"line":40,"column":35}},{"generated":{"line":112,"column":0},"source":"components/App.js","original":{"line":40,"column":8}},{"generated":{"line":112,"column":8},"source":"components/App.js","original":{"line":36,"column":6}},{"generated":{"line":112,"column":9},"source":"components/App.js","original":{"line":35,"column":4}},{"generated":{"line":113,"column":0},"source":"components/App.js","original":{"line":43,"column":3}},{"name":"Component","generated":{"line":117,"column":2},"source":"components/App.js","original":{"line":8,"column":18}},{"name":"App","generated":{"line":119,"column":15},"source":"components/App.js","original":{"line":46,"column":15}}],"sources":{"components/App.js":"import React, { Component } from 'react';\nimport Search from './Search';\nimport Artist from './Artist';\nimport Tracks from './Tracks';\n\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\n\nclass App extends Component{\n  state = { artist: null, tracks: [] };\n\n  componentDidMount() {\n    this.searchArtist('tasha cobbs');\n  }\n\n  searchArtist = artistQuery => {\n    fetch(`${API_ADDRESS}/artist/${artistQuery}`)\n    .then(response => response.json())\n    .then(json => {\n\n      if (json.artists.total > 0) {\n         const artist = json.artists.items[0];\n\n         this.setState({ artist });\n\n         fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n         .then(response => response.json())\n         .then(json => this.setState({ tracks: json.tracks }))\n         .catch(error => alert(error.message));\n      }\n    })\n    .catch(error => alert(error.message));\n  }\n\n  render() {\n    return (\n      <div>\n        <h2>Music Professor</h2>\n        <Search searchArtist={this.searchArtist}/>\n        <Artist artist={this.state.artist} />\n        <Tracks tracks={this.state.tracks} />\n      </div>\n    );\n  }\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"9c99f7b43d3b4943cf3da306955d9de9","cacheData":{"env":{}}}